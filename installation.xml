<?xml version="1.0" encoding="UTF-8"?>

<install>
    <serviceproviders>
        <provider name="Logger"></provider>
        <provider name="Session"></provider>
        <provider name="Cookie"></provider>
        <provider name="Cache">
            <share>RedisCache</share>
        </provider>
        <provider name="Redis">
            <file name="redis.ini" path="/config/dev"></file>
        </provider>
        <provider name="Memcached">
            <file name="memcached.ini" path="/config/dev"></file>
        </provider>
        <provider name="Mongo">
            <file name="mongo.ini" path="/config/dev"></file>
        </provider>
        <provider name="MongoDB">
            <file name="mongodb.ini" path="/config/dev"></file>
        </provider>
        <provider name="SubRequest"></provider>
        <provider name="Amqp">
            <share>AmqpConnector</share>
        </provider>
        <provider name="Doctrine">
            <file name="database.ini" path="/config/dev"></file>
            <file name="cli-config.php" path="/"></file>
            <file name="doctrine" path="/"></file>
        </provider>
    </serviceproviders>
</install>